

<div class="row text-center">
	<div class="col-12 col-sm">
	<h1>Check the calendar for trip updates</h1>

	<%= month_calendar events: @meetings do |date, meetings| %>
  	<%= date %>

  	<% meetings.each do |meeting| %>
    <div>
      <%= meeting.name %>
    </div>
</div>
  <% end %>
<% end %>



